#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int t; 
    scanf("%d",&t);
    for(int a0 = 1; a0 <= t; a0++){
        long n,n1,n2,n3,res=0; 
        scanf("%ld",&n);
        n=n-1;
        n1=n/3;
        n2=n/5;
        n3=n/15;
        res=(3*((n1*(n1+1))/2))+(5*((n2*(n2+1))/2))-(15*((n3*(n3+1))/2));
        printf("%ld\n",res);
    }
    return 0;
}
